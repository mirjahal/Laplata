{
    "name": "Laplata",
    "description": "Aplicação web para controle de finanças pessoais, construído para aprendizado do Framework Lumen.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
		"php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
		"mockery/mockery": "0.9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
		"fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
